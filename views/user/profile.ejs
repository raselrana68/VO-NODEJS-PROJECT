<% include header%>
<table align="center" style="font-size: 20px">
	<tr>
		<td>
			<h2>My Profile</h2>
			<% if(messages.info)
			 	{ %> <p style="color:green;"> <%= messages.info %> </p> <%} 
	 		%>
		</td>
	</tr>
	<tr>
		<td>
			<form method="post">
				<table border="1">
					<tr>
						<td>FULL NAME: </td>
						<td><%= user.fullname %></td>
					</tr>
					<tr>
						<td>EMAIL: </td>
						<td><%= user.email %></td>
					</tr>
					<tr>
						<td>USERNAME: </td>
						<td><%= user.username %></td>
					</tr>
					<tr>
						<td>TYPE: </td>
						<td><%= user.type %></td>
					</tr>
					<tr>
						<td colspan="2">
							<br/>
							<center>
								<a href="/userHome">Back</a> | 
								<a href="/userHome/edit/<%= user.userId %>"'>Edit</a>
							</center>
						</td>
					</tr>
				</table>
			</form>

		</td>
	</tr>

</table>

